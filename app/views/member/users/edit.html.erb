  <div class="container">
    <div class="row no-gutters">
      <div class="col-md-5 col-sm-10 mx-2 mx-sm-auto text-center shadow-lg rounded px-0 my-5">
        <h5 class="text-dark bg-light p-4 mb-0">プロフィール編集</h5>
          <div class="form-body p-4">
          <%= form_with model: @member, url: user_path(current_member) do |f| %>
            <label for="inputImage">ProfileImage</label> <br>
            <!--プロフィール画像-->
            <%= f.file_field :image, placeholder: "プロフィール画像", accept: "image/*" %> <br>
          <div class="p-2">
            <label for="inputName">last_name</label> <br>
            <!--名字-->
            <%= f.text_field :last_name, autofocus: true, id:"inputName", placeholder:"姓"%> <br>
          </div>
          <div class="p-2">
            <label for="inputName">first_name</label> <br>
            <!--名前-->
            <%= f.text_field :first_name, autofocus: true, id:"inputName", placeholder:"名"%> <br>
          </div>
          <div class="p-2">
            <!--ボタン-->
            <%= f.submit "変更を保存", class: 'btn btn-sm btn-success col-md-6' %>
          </div>
          <div class="p-2">
            <!--退会ボタン-->
            <%= link_to "退会する", users_unsubscribe_users_path, class: 'btn btn-sm btn-success col-md-6' %>
          </div>

          <% end %>
          </div>
      </div>